		"symfony/debug": "~2.3,<2.7",
		"symfony/dom-crawler": "~2.3,<2.7",
		"symfony/finder": "~2.3,<2.7",
		"symfony/monolog-bridge": "~2.3,<2.7",
		"symfony/options-resolver": "~2.3,<2.7",
